#include <iostream>
using namespace std;
class slution{
	int maxdepth;
	void dfs(Treenode* root, int depth) {
		if (depth > maxdepth) {
			maxdepth = depth;
		}
		if (root) {
			dfs(root->left, depth + 1);
			dfs(root->right, depth + 1);
		}
	}
public:
	int caldepth(Treenode* root) {
		maxdepth = 0;
		dfs(root, 0);
		return maxdepth;
	}
}
