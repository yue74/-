#include <iostream>
using namespace std;
class slution{
	int maxdepth;
	void dfs(Treenode* root, int depth) {
		if (root) {
			depth++;
			if (depth > maxdepth) {
				maxdepth = depth;
			}
			dfs(root->left, depth);
			dfs(root->right, depth);
		}
	}
public:
	int caldepth(Treenode* root) {
		maxdepth = 0;
		dfs(root, 0);
		return maxdepth;
	}
}
